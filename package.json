    "build:webview": "node scripts/webview/build.js",
    "build": "yarn build:plugin && yarn build:webview",
    "bundle": "yarn build && node scripts/plugin/bundle.js",
    "lint:plugin": "eslint ./src/plugin/ -c ./config/plugin/eslint.js || true",
